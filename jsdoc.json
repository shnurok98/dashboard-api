{
	"source": {
		 "include": ["./models", "./routes", "./middleware"],
		 "includePattern": ".js$",
		 "excludePattern": "(node_modules/|docs)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		 "encoding": "utf8",
		 "destination": "./docs/",
		 "recurse": true,
		 "readme": "./README.md"
	}
}